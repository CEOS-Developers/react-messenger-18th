{
  "compilerOptions": {
    "target": "es5",
    "lib": ["DOM", "ES6", "ES2017", "ES2018", "ES2019", "ES2020"],
    "jsx": "react",
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "include": ["src"]
}
